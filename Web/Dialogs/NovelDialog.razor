<MudButton OnClick="Back">Back</MudButton>
<MudDialog>
    <DialogContent>
        Character Names @novelId
    </DialogContent>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; } = null!;
    [Parameter] public string novelId { get; set; }


    void Back() => MudDialog.Cancel();
}
